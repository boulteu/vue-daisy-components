{
  "name": "vue-daisy-components",
  "version": "0.2.0",
  "description": "Beautiful and optimized Vue 3 components styled with Tailwind CSS and DaisyUI.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint src --ext .vue,.js,.ts,.jsx,.tsx --fix"
  },
  "keywords": [
    "vue",
    "vue3",
    "datatable",
    "tailwind",
    "daisyui",
    "table",
    "pagination",
    "vue-component",
    "typescript",
    "optimized",
    "export",
    "selection"
  ],
  "author": "Boulteu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/boulteu/vue-daisy-components.git"
  },
  "peerDependencies": {
    "tailwindcss": "^4.1.12",
    "daisyui": "^5.0.50",
    "vue": "^3.5.18"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "terser": "^5.43.1",
    "typescript": "^5.2.0",
    "vite": "^5.0.0",
    "vite-svg-loader": "^5.1.0",
    "vue-tsc": "^3.0.5"
  }
}
